<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.utaskdeo.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment2"
            app:destination="@id/secondFragment" />
        <action
            android:id="@+id/action_firstFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
        <action
            android:id="@+id/action_firstFragment_to_fourthFragment"
            app:destination="@id/fourthFragment" />
        <action
            android:id="@+id/action_firstFragment_to_thirdFragment2"
            app:destination="@id/thirdFragment" />
        <action
            android:id="@+id/action_firstFragment_to_fifthFragment"
            app:destination="@id/fifthFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.utaskdeo.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" />
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.utaskdeo.ThirdFragment"
        android:label="ThirdFragment" >
        <action
            android:id="@+id/action_thirdFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.example.utaskdeo.FourthFragment"
        android:label="FourthFragment" />
    <fragment
        android:id="@+id/fifthFragment"
        android:name="com.example.utaskdeo.FifthFragment"
        android:label="FifthFragment" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.utaskdeo.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

</navigation>